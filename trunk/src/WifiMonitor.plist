<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.wifi_monitor</string>
		
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/wifi_monitor</string>
	</array>
	
	<key>LimitLoadToSessionType</key>
	<string>Aqua</string>
	
	<key>RunAtLoad</key>
	<true/>
	
	<key>OnDemand</key>
	<false/>
	
	<key>KeepAlive</key>
	<true/>
</dict>
</plist>
